			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>控件id，务必确定其唯一性</description>
		</attribute>
		<attribute>
			<name>value</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>value(数据格式 id,id,id*name,name,name)</description>
		</attribute>
		<attribute>
			<name>code</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>部门或公司编号(用于限定公司或部门),不设置时为全省范围选择</description>
		</attribute>
		<attribute>
			<name>type</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>部门或公司类型(要结合code进行使用)，输入值为organization或company</description>
		</attribute>
		<attribute>
			<name>selectType</name>
			<required>false</required>
