	<tag>
		<name>proLogsV1.0</name>
		<tag-class>com.nuchina.mycontrol.tld.ProcessLog</tag-class>
		<body-content>empty</body-content>
		<description>采用通用日志控件显示日志时，日志表必须符合字段规范</description>
		<attribute>
			<name>id</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>tableName</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>日志信息从哪个实体表获取</description>
		</attribute>
		<attribute>
			<name>appId</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>多个流程实例组实现相应业务时使用，不存在此业务时请赋值0</description>
		</attribute>
		<attribute>
			<name>proId</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>流程实例ID，当appId为非0时，proID请赋值0</description>
		</attribute>
		<attribute>
			<name>type</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>显示审批日志的类型：all(全部日志)、获取输入类型(1,2)， 默认为all</description>
		</attribute>
		<attribute>
			<name>showhead</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>空数据是否显示头部：true、false， 默认为true</description>
		</attribute>
		<attribute>
			<name>shownofinish</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>是否显示正在处理中的日志：true、false， 默认为true</description>
		</attribute>
		<attribute>
			<name>showlogs</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>是否展开日志信息：true、false， 默认为true</description>
		</attribute>
		<attribute>
			<name>showrounds</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>是否显示审批轮次列：true、false， 默认为true</description>
		</attribute>			
	</tag>
	<tag>
		<name>select</name>
		<tag-class>com.nuchina.mycontrol.tld.DicSelect</tag-class>
		<body-content>empty</body-content>
		<description>采用字典表控件时表中必须有字段CFG_TYPE</description>
		<attribute>
			<name>id</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>name</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>tableName</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>字典表名称</description>
		</attribute>
		<attribute>
			<name>cfgType</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>字典类型</description>
		</attribute>
		<attribute>
			<name>title</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description></description>
		</attribute>
		<attribute>
			<name>cssStyle</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description></description>
		</attribute>
		<attribute>
			<name>cssClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description></description>
		</attribute>
		<attribute>
			<name>disabled</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>true;false</description>
		</attribute>
		<attribute>
			<name>headerKey</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>第一列选项的值</description>
		</attribute>
		<attribute>
			<name>headerValue</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>第一列选项的描述</description>
		</attribute>
		<attribute>
			<name>listKey</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>选项的值对应的列名</description>
		</attribute>
		<attribute>
			<name>listValue</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>选项的描述对应的列名</description>
		</attribute>
		<attribute>
			<name>value</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description></description>
		</attribute>
		<attribute>
			<name>onchange</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description></description>
		</attribute>
		<attribute>
			<name>onclick</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description></description>
		</attribute>
		<attribute>
			<name>priority</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>选项排列顺序的字段名+ASC/DESC,默认PRIORITY字段升序</description>
		</attribute>
		<attribute>
			<name>systemid</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>字典数据所属系统ID，为空时不进行筛选</description>
		</attribute>
		<attribute>
			<name>onlyShow</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>默认为false,为true时不生成select控件，而根据textShow属性生成input或纯文本</description>
		</attribute>
		<attribute>
			<name>textShow</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>onlyShow属性为true的场合,为true时生成只读input控件,为false时生成纯文本。默认值:false</description>
		</attribute>
	</tag>
</taglib>