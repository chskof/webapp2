<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
    "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">
<taglib>
	<tlib-version>1.0</tlib-version>
	<jsp-version>1.2</jsp-version>
	<short-name>myControl</short-name>
	<uri>cmuc/tlds/myControl</uri>
	<tag>
		<name>SelectOrgByListV1.0</name>
		<tag-class>com.nuchina.mycontrol.tld.SelectOrgNew</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<name>valueList</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>value(数据格式 List)</description>
		</attribute>
		<attribute>
			<name>valueType</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>显示数据类型</description>
		</attribute>	
		<attribute>
			<name>onlyshow</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>仅显示数据，不可选择</description>
		</attribute>
		<attribute>
			<name>code</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>部门或公司编号(用于限定公司或部门),不设置时为全省范围选择</description>
		</attribute>
		<attribute>
			<name>type</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>部门或公司类型(要结合code进行使用)，输入值为organization或company</description>
		</attribute>
		<attribute>
			<name>selectType</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>选择类型(person,organization,company)，默认为：person</description>
		</attribute>
		<attribute>
			<name>selectNum</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>选择数量，输入大于0的数字,默认为不限制</description>
		</attribute>
		<attribute>
			<name>openUnit</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>默认展开的机构</description>
		</attribute>
		<attribute>
			<name>condifs</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>过滤条件：在或不在指定人员，部门，公司内,格式pin(id,id)或pnotin(id,id),oin(id,id)或onotin(id,id),cin(id,id)或cnotin(id,id)</description>
		</attribute>
		<attribute>
			<name>idinputName</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>对象id文本框name属性</description>
		</attribute>
		<attribute>
			<name>nameinputName</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>对象name文本框name属性</description>
		</attribute>
		<attribute>
			<name>typeinputName</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>对象type文本框name属性</description>
		</attribute>
		<attribute>
			<name>showGroup</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>是否显示群组 ：true或false，默认为：true</description>
		</attribute>		
		<attribute>
			<name>showbtn</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>是否显示按钮 ：true或false，默认为：false</description>
		</attribute>
		<attribute>
			<name>btnvalue</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>按钮文本</description>
		</attribute>
		<attribute>
			<name>btnClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>按钮样式</description>
		</attribute>
		<attribute>
			<name>txtClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>文本框样式</description>
		</attribute>
		<attribute>
			<name>toolTip</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>文本框显示的toolTip</description>
		</attribute>
		<attribute>
			<name>txtType</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>文本框类型：input或textarea,默认为：input</description>
		</attribute>
		<attribute>
			<name>readonly</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>文本框是否只读：true或false,默认为：true</description>
		</attribute>
		<attribute>
			<name>groupCode</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>群组code</description>
		</attribute>
		<attribute>
			<name>onSelectChange</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>文本框改变事件</description>
		</attribute>
	</tag>
	<tag>
		<name>SelectOrgV1.0</name>
		<tag-class>com.nuchina.webapp1.exam.cmuc.SelectOrg</tag-class>
		<body-content>empty</body-content>
		<attribute>
